@model ProvinceDto

<div class="card card-secondary">
    <div class="card-header">
        <h3 class="card-title">İl EKle</h3>
        <a href="/Vehicle/Creating"><b>Admin</b>LTE</a>
    </div>
    <form class="form-horizontal" action="/Province/PropAdd" method="post">
        @Html.HiddenFor(p=>p.Id)
        <div class="card-body">
            <div class="form-group row">

                <label for="rolename" class="col-sm-2 col-form-label">İl Ekle</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(p=>p.Name)
                </div>
            </div>
        </div>
        <div class="card-footer">
            <a href=" /Province/Province"><b>Vazgeç</b></a>
            <button type="submit" class="btn btn-info float-right">Kaydet</button>
        </div>
    </form>
</div>
