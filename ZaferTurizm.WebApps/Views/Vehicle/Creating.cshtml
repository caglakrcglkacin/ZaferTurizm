@model VehicleMakeDto

<div class="card card-secondary">
    <div class="card-header">
        <h3 class="card-title">Role Ekle</h3>
        <a href="/Vehicle/Creating"><b>Admin</b>LTE</a>
    </div>
    <form class="form-horizontal" action="/Vehicle/Creating" method="post">
        <div class="card-body">
            <div class="form-group row">
                <label for="rolename" class="col-sm-2 col-form-label">Role Adı</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(p=>p.Name)
                </div>
            </div>
        </div>
        <div class="card-footer">
            <button type="submit" class="btn btn-dark float-right">Vazgeç</button>
            <button type="submit" class="btn btn-info float-right">Kaydet</button>
        </div>
    </form>
</div>

